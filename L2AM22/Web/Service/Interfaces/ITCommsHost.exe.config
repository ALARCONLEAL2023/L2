            <publisher enable="false" name="Push" mode="http://schemas.xmlsoap.org/ws/2004/08/eventing/DeliveryModes/Push" type="RKiss.WSEventing.Adapters.PublisherTester, WSEventing" desc="hello world" />
            <publisher enable="false" name="Pull" mode="http://schemas.xmlsoap.org/ws/2004/08/eventing/DeliveryModes/Pull" type="xyz_0" />
            <publisher enable="false" name="Wrapp" mode="http://schemas.xmlsoap.org/ws/2004/08/eventing/DeliveryModes/Wrapp" type="xyz_1" />
            <publisher enable="false" name="PushWithAck" mode="http://schemas.xmlsoap.org/ws/2004/08/eventing/DeliveryModes/PushWithAck" type="xyz_2" />
